{
  "name": "dogecoinjs-lib",
  "version": "0.0.1",
  "description": "dogecoin javascript library for web, mobile and server.",
  "keywords": [
    "dogecoin",
    "dogecoinjs-lib",
    "dogecoin-js",
    "dogecoinjs",
    "crypto",
    "crytography",
    "react",
    "react-native"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ChrisCho-H/dogecoinjs-lib.git"
  },
  "main": "./lib/cjs/index.cjs",
  "scripts": {
    "compile": "tsc -p tsconfig.esm.json",
    "compile:cjs": "tsc -p tsconfig.cjs.json",
    "test": "cd lib/esm && npm run test",
    "test:cjs": "cd lib/cjs && npm run test",
    "lint": "eslint \"src/**/*.ts\" \"test/**/*.ts\" --ignore-path ./.eslintignore",
    "lint:fix": "eslint --fix \"src/**/*.ts\" \"test/**/*.ts\" --ignore-path ./.eslintignore",
    "format": "prettier \"src/**/*.ts\" \"test/**/*.ts\" --ignore-path ./.prettierignore",
    "format:fix": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" --ignore-path ./.prettierignore"
  },
  "author": "Chris Hyunhum Cho <hyeonheum@naver.com>",
  "dependencies": {
    "@noble/curves": "^1.2.0",
    "@noble/hashes": "^1.3.2",
    "bs58": "^5.0.0"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "c8": "^7.12.0",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "mocha": "^10.1.0",
    "prettier": "^2.8.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "exports": {
    ".": {
      "require": {
        "types": "./lib/cjs/index.d.cts",
        "default": "./lib/cjs/index.cjs"
      },
      "import": {
        "types": "./lib/esm/index.d.mts",
        "default": "./lib/esm/index.mjs"
      }
    }
  },
  "module": "./lib/esm/index.mjs",
  "types": "./lib/esm/index.d.mts",
  "sideEffects": false
}
